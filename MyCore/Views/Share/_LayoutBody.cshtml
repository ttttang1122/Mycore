            font-weight: 400;
            line-height: 20px;
            text-align: right;
            font-size: 12px;
        }

        .layui-form-select dl dd, .layui-form-select dl dt {
            line-height: 24px;
        }

        .layui-form-item .layui-input-inline {
            font-size: 12px;
        }

        .layui-input, .layui-select, .layui-textarea {
            height: 25px;
        }

        .layui-unselect dl {
            max-height: 200px;
        } 
